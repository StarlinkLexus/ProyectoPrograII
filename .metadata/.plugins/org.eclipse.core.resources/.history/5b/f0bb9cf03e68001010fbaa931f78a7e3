package ProyectoApp2.SistemaBancario;

public class Cuenta {
int numero;
double saldo;
public Cuenta(int numero, double saldo) {
	super();
	this.numero = numero;
	this.saldo = saldo;
}
public int getNumero() {
	return numero;
}
public void setNumero(int numero) {
	this.numero = numero;
}
public double getSaldo() {
	return saldo;
}
public void setSaldo(double saldo) {
	this.saldo = saldo;
}

public 

























}
